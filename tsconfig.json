{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@trnja/api/*": ["./packages/api/src/*"],
      "@trnja/api-types/*": ["./packages/api-types/src/*"],
      "@trnja/evm/*": ["./packages/evm/src/*"]
    }
  }
}
